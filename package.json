{
  "name": "typescript-boilerplate",
  "version": "0.0.1",
  "description": "typescript boilerplate",
  "scripts": {
    "clean": "rimraf dist",
    "watch": "webpack --watch",
    "prebuild": "npm run clean",
    "build": "webpack",
    "build:prod": "webpack --progress -p",
    "prestart": "npm run build",
    "server": "nodemon dist/server/index.js",
    "start": "npm-run-all --parallel server watch",
    "predebug": "npm run build",
    "debug": "node --debug-brk dist/server/index.js",
    "deploy": "surge --project ./dist/client --domain https://examplesite.surge.sh",
    "undeploy": "surge teardown --domain https://examplesite.surge.sh"
  },
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "copy-webpack-plugin": "^3.0.1",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "surge": "^0.18.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-merge": "^0.14.1"
  }
}

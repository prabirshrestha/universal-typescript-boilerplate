{
  "name": "typescript-boilerplate",
  "version": "0.0.1",
  "description": "typescript boilerplate",
  "scripts": {
    "clean": "rimraf build",
    "watch": "webpack --watch",
    "prebuild": "npm run clean",
    "build": "webpack",
    "build:prod": "webpack --progress -p",
    "prestart": "npm run build",
    "server": "nodemon build/server.js",
    "start": "npm-run-all --parallel server watch",
    "predebug": "npm run build",
    "debug": "node --debug-brk build/server.js",
    "deploy": "surge --project ./build/public --domain https://examplesite.surge.sh",
    "undeploy": "surge teardown --domain https://examplesite.surge.sh",
    "install:react": "npm i --save react react-dom react-router@next mobx mobx-react mobx-react-devtools && npm i --save-dev @types/react @types/react-dom https://github.com/danfma/typed-react-router"
  },
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "tslib": "^1.5.0"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.34",
    "@types/isomorphic-fetch": "0.0.32",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-style-loader": "^1.1.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "postcss-assets": "^4.1.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.14.0",
    "stylelint-webpack-plugin": "^0.5.0",
    "surge": "^0.18.0",
    "ts-loader": "^1.3.3",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^2.3.0",
    "typescript": "^2.1.5",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^2.2.1",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^2.0.0"
  }
}
